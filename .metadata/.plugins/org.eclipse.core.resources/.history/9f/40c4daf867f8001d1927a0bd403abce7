package objectarray;

public class DogMain {
	
	public static void main(String[] args) {
		// 강아지 3마리 생성하기 - 배열 공간 생성
		Dog[] dogAraay = new Dog[3];
		dogAraay[0] = new Dog();
		
		dogAraay[0].setName("코코");
		dogAraay[0].setType("코카스파니엘");
		
		
		
		for (Dog dog : dogAraay) {
			if (dog.getName() != null){
				System.out.println(dog.getName());
			}
		}
		
	}
	
}
