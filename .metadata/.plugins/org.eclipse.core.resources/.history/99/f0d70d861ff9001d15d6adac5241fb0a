package arraylist;

import java.util.ArrayList;

public class ArrayList2 {

	public static void main(String[] args) {
		// 정수를 저장할 어레이리스트 생성
		ArrayList<Integer> numberList = new ArrayList<>();
		
		numberList.add(1);
		numberList.add(2);
		numberList.add(3);
		numberList.add(4);
		numberList.add(5);
		
		// 요소의 합계
		int sumV = 0;
		for (int i = 0; i < numberList.size(); i++) {
			numberList.get(i);
			
		}
		
	}

}
