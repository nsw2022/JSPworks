package inheritance.airplane;

public class AirPlanMain {

	public static void main(String[] args) {
		// SuperSonicAirPlane 생성
		SuperSonicAirPlane sa = new SuperSonicAirPlane();
		
		sa.takeOff();
		sa.fly();
		sa.land();
		System.out.println();
		
		sa.takeOff();
		sa.flymode=SuperSonicAirPlane.SUPERSONIC;
		sa.fly();
		sa.land();

	}

}
