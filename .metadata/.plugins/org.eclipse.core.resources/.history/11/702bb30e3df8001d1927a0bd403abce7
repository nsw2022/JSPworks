package methods;



public class OneUp2 {
	
	static int x = 0; // static이 붙으면 정적변수
	
	public static int oneUp() {
		x++;
		return x;
	}
	
	public static void main(String[] args) {
		
		System.out.println(oneUp());//1
		System.out.println(oneUp());//1
		System.out.println(oneUp());//1
		
		
		
	}

}
